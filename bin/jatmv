#!/usr/bin/env perl -w

use v5.20;
use warnings;
use utf8;

use File::Find;
use Data::Dump;
use File::Basename;
use POSIX qw(strftime);
use YAML::Syck;
use File::Path qw(make_path);
use File::Copy qw(cp);

$YAML::Syck::ImplicitUnicode = 1;

# Other imports:
# https://medium.com/@theory/the-watch-is-you-ef0e416ce0f9 (essays)
# https://medium.com/on-culture/11cfd5b919f6 (essays)
# https://theory.svbtle.com/the-xoxo-women-rocked-it (conferences)

my %convert = catconv();
my %cats =  catmap();
my %tagmap = tagmap();
my %desc_for = descmap();
my %linkmap = linkmap();

my $src = 'entries';
my $dst = shift || die "Usage: $0 [DEST DIR]\n";

for my $dir ($src, $dst) {
    die "$dir does not exist\n" unless -e $dir;
    die "$dir is not a directory\n" unless -d $dir;
}

find +{
    no_chdir => 1,
    wanted => sub {
        return if -d;
        my $bn = basename $_;
        # Skip files in the root directory.
        return if $File::Find::name eq "$src/$bn";
        my ($data, $body) = parse($File::Find::name);

        # Append via.
        $body .= "\n<p>(Via $data->{via})</p>\n" if $data->{via};

        # Append past link.
        $body .= qq{\n<p class="past"><small>Looking for the comments? Try the <a rel="nofollow" href="//past.justatheory.com$data->{uri}">old layout</a>.</small></p>\n\n};

        # Get the slug.
        $data->{uri} =~ s/-mony-/-money-/;
        my $slug = basename $data->{uri};
        $slug =~ s/[.][^.]+$//;
        $slug =~ s/_+/-/g;

        # Fixups.
        if ($data->{dir} eq 'bricolage') {
            $slug = "bricolage-$slug" if $slug !~ /bricolage/;
        } elsif ($slug eq 'projectors') {
            $slug = 'conference-projectors';
        } elsif ($data->{uri} =~ m{mysql/replace}) {
            $slug = "mysql-$slug";
        } elsif ($data->{uri} =~ m{database/change}) {
            $slug = "database-$slug";
        } elsif ($data->{uri} =~ m{database/ordering}) {
            $slug = "sql-$slug";
        } elsif ($data->{uri} =~ m{database/sqlite/}) {
            $slug = "sqlite-$slug";
        } elsif ($data->{dir} eq 'js') {
            $slug = "js-$slug" if $slug !~ /^test|js/;
        } elsif ($data->{uri} =~ m{/oscon/}) {
            $slug = "oscon-$slug" if $slug !~ /oscon/;
        } elsif ($slug eq 'another-short-trip') {
            $slug = 'another-short-alamosa-trip';
        } elsif ($slug eq 'june-2004') {
            $slug = 'alamosa-visit';
        } elsif ($slug eq 'arrival') {
            $slug = 'arrived-in-france';
        } elsif ($slug eq 'departure') {
            $slug = 'departed-france';
        } elsif ($slug eq 'more-sun') {
            $slug= 'french-sun';
        } elsif ($slug eq 'embed-test') {
            $slug = "html-$slug";
        } elsif ($slug eq 'introducing-mysql') {
            $slug = "introducing-mytap";
        }

        # Determine destination directory and file.
        my $dir = "$dst/past/$data->{dir}";
        my $fn = "$dir/$slug.md";

        # Determine if there are non-HTML local links.
        if (my $links = $linkmap{$slug}) {
            $dir = "$dir/$slug";
            $fn = "$dir/index.md";
            my ($y, $m) = $data->{date} =~ /^(\d{4})-(\d\d)/;
            for my $link (@{ $links }) {
                my $bn = basename $link;
                my $new = "$y/$m/$slug/$bn";
                $body =~ s/\Q$link/$new/g;
                say "cp $link $dir";
            }
        }
        # Write out new file.
        make_path $dir;
        open my $fh, '>:encoding(UTF-8)', $fn or die "Cannot open $fn: $!\n";
        say {$fh} join "", (
            Dump({
                title => $data->{title},
                date => $data->{date},
                slug => $slug,
                ($data->{desc} ? (description => $desc_for{$File::Find::name}) : ()),
            }),
            "aliases: [$data->{uri}]\n",
            "tags: [" . join(', ', @{ $data->{tags} }) . "]\n",
            "---\n\n",
            $body,
        );
    },
}, $src;

sub parse {
    my $fn = shift;
    open my $fh,  '<:encoding(UTF-8)', $fn or die "Cannot open $fn: $!\n";
    my $title = <$fh>;
    say $fn if !defined $title;
    chomp $title;
    $title =~ s{<q>([^<]+)</q>}{“$1”};
    $title =~ s/<[^>]+>//g;
    say $title if $title =~ /</;


    my $data = { title => $title };
    while (<$fh>) {
        last if m/^\w*$/;
        chomp;
        my ($key, $line) = split /\s*:\s*/, $_, 2;
        $data->{$key} = $line;
    }
    for my $bad (qw(names word wors)) {
        $data->{'meta-keywords'} = delete $data->{"meta-key$bad"}
            if exists $data->{"meta-key$bad"};
    }
    delete $data->{$_} for qw(meta-tags meta-readmore);
    $data->{desc} = delete $data->{description};
    for my $desc (qw(teaser description)) {
        $data->{desc} = delete $data->{"meta-$desc"} if exists $data->{"meta-$desc"};
    }
    $data->{desc} =~ s/<[^>]+>//g if $data->{desc};
    $data->{via} = delete $data->{'meta-via'} if exists $data->{'meta-via'};

    $data->{date} = time_for($fn);
    $data->{uri} = $fn =~ s/\Q$src//r;
    $data->{uri} =~ s/[.]txt$/.html/;

    my ($dir, @tags) = cats_for($data->{uri});
    if (my $t = delete $data->{'meta-keywords'}) {
        push @tags => map { @{ $tagmap{$_} || [] } } split /,\s*/, $t;
    } elsif ($t = $data->{tags}) {
        for my $tag (split /\s+/, $t) {
            $tag =~ s/[+]/ /;
            push @tags, @{ $tagmap{$tag} || [] };
        }
    }

    $data->{tags} = tags_for(@tags);
    $data->{dir} = $dir;
    return $data, join '', <$fh>;
}

sub time_for($) {
    my $file = shift;
    if (my $time = `git -c log.showsignature=false log -1 --format='%at' --diff-filter=A $file`) {
        chomp $time;
        return strftime "%Y-%m-%dT%H:%M:%SZ", gmtime $time;
    }
    return strftime "%Y-%m-%dT%H:%M:%SZ", gmtime stat($file)->mtime;
}

sub cats_for {
    my $uri = substr shift =~ s/[.][^.]+$//r, 1;
    my $path = $uri =~ s{/[^/]+$}{}r;

    if (my $c = $convert{$path}) {
        if ($uri =~ $c->{match}) {
            return $c->{dir}, $c->{topic};
        }
    }

    if (my $spec = $cats{$uri}) {
        return @{$spec};
    }

    my $spec = $cats{$path} || die "Unknown category for $uri\n";
    return @{$spec};
}

sub tags_for {
    my %seen;
    return [ grep { !$seen{lc $_}++ } @_ ];
}

